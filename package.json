{
  "name": "react-ssr-nat-geo-potd",
  "homepage": "https://olezt.github.io/react-ssr-nat-geo-potd",
  "version": "1.0.0",
  "engines": {
    "node": "12.12.0"
  },
  "description": "NatGeo photo of the day",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch dist --exec node dist/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "heroku": "npm-run-all --parallel heroku:*",
    "heroku:server": "nodemon --exec node dist/bundle.js",
    "heroku:build-server": "webpack --config webpack.server.js",
    "heroku:build-client": "webpack --config webpack.client.js",
    "start": "npm run heroku"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "css-loader": "3.5.3",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "lodash": "4.17.4",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "6.0.0",
    "react-redux": "5.0.6",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "4.0.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0",
    "react-dates": "21.8.0",
    "moment": "2.24.0"
  }
}
